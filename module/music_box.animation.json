        "animation.music_box.setblock": {
            "loop": false,
            "animation_length": 0.1,
            "timeline": {
                "0.05": "/setblock ~ ~ ~ mb:dummy_block"
            }
        },
        "animation.music_box.on_loop": {
            "loop": true,
            "animation_length": 0.05,
            "timeline": {
                "0": [
                    "/execute if block ^ ^ ^-1 gold_block run tag @s add looped",
                    "/execute if block ^ ^ ^-1 gold_block run event entity @s mb:should_loop",
                    "/execute unless block ^ ^ ^-1 gold_block run tag @s remove looped",
                    "/execute unless block ^ ^ ^-1 gold_block run event entity @s mb:should_not_loop",
                    "/execute unless block ~ ~1 ~ air run event entity @s mb:spill",
                    "/execute unless block ~ ~1 ~ air run stopsound @a",
                    "/execute unless block ~ ~ ~ mb:dummy_block run event entity @s mb:destroyed"
                ]
            }
        },
